{
  "metadata": [
    {
      "src": [
        {
          "cwd": "..",
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "src/Dynamo/**",
            "src/Revit/**",
            "src/Unity/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md",
          "toc.yml",
          "*.md",
          "api/**",
          "Elements/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "favicon.ico"
        ]
      },
      {
        "files": [
          "js/**"
        ]
      },
      {
        "src": "../test/Elements.Tests/bin/debug/netcoreapp2.1/models/",
        "files": [
          "Elements_**.glb",
          "Elements_ElementInstance.glb",
          "Elements_Geometry_Arc.glb",
          "Elements_Geometry_Bezier.glb",
          "Elements_Geometry_Contour.glb",
          "Elements_Geometry_Line.glb",
          "Elements_Geometry_Polygon.glb",
          "Elements_Geometry_Polyline.glb",
          "Elements_Geometry_Transform.glb",
          "Elements_ModelCurve.glb",
          "Elements_ModelPoints.glb",
          "Elements_Analysis_AnalysisMesh.glb",
          "Elements_Analysis_ColorScale.glb",
          "Elements_Column.glb",
          "Elements_Beam.glb",
          "Elements_Floor.glb",
          "Elements_Spatial_Grid1d.glb",
          "Elements_Mass.glb",
          "Elements_ModelCurve.glb",
          "Elements_Panel.glb",
          "Elements_Space.glb",
          "Elements_Topography.glb",
          "Elements_StandardWall.glb",
          "Elements_ImportMeshElement.glb"
        ],
        "dest": "models"
      }
    ],
    "overwrite": [
      {
        "files": [],
        "exclude": [
          "obj/**",
          "../docs/**"
        ]
      }
    ],
    "dest": "../docs/",
    "globalMetadata": {
      "_appTitle": "Hypar Docs",
      "_appFooter": "Copyright Â© Hypar Inc., 2019",
      "_disableContribution": true,
      "_enableSearch": true,
      "_disableBreadcrumb": true,
      "_disableFooter": true,
      "_appFaviconPath": "favicon.ico"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "./template/hypar"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}
