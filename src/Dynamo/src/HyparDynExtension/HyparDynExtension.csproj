<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DynamoVisualProgramming.DynamoServices" Version="2.5.0.7432" >
      <Private>False</Private>
    </PackageReference>
    <PackageReference Include="DynamoVisualProgramming.WpfUILibrary" Version="2.5.0.7432" >
      <Private>False</Private>
    </PackageReference>
  </ItemGroup>
  
  <Target Name="CopyToPackage" AfterTargets="Publish">
    <Copy SourceFiles="$(TargetDir)$(TargetName).dll" DestinationFolder="..\..\deploy\HyparDyn\bin" ContinueOnError="true" />
    <Copy SourceFiles="HyparDyn_ExtensionDefinition.xml" DestinationFolder="..\..\deploy\HyparDyn\extra" ContinueOnError="true" />
  </Target>

</Project>
