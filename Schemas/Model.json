{
    "$id": "https://hypar.io/schemas/elements/Model.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A container of elements.",
    "title": "Model",
    "type": "object",
    "required": ["Elements", "Materials", "Profiles", "ElementTypes"],
    "properties": {
        "Elements": {
            "type": ["array","null"],
            "items": {
                "$ref": "Element.json"
            },
            "description": "A dictionary of elements in the model keyed by their id."
        },
        "Materials": {
            "type": ["array","null"],
            "items": {
                "$ref": "Material.json"
            },
            "description": "A dictionary of materials in the model keyed by their id."
        },
        "Profiles": {
            "type": ["array","null"],
            "items": {
                "$ref": "Geometry/Profile.json"
            },
            "description": "A dictionary of profiles in the model keyed by their id."
        },
        "ElementTypes": {
            "type": ["array","null"],
            "items": {
                "$ref": "ElementTypes/ElementType.json"
            },
            "description": "A dictionary of element types in the model keyed by their id."
        },
        "Origin": {
            "description": "The origin of the model.",
            "$ref": "GeoJSON/Position.json"
        }
    },
    "additionalProperties": false
}