{
    "$id": "https://raw.githubusercontent.com/hypar-io/Elements/representation-add-representations/Schemas/Model.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A container of elements.",
    "title": "Model",
    "x-namespace": "Elements",
    "type": "object",
    "required": [
        "Transform",
        "Elements"
    ],
    "properties": {
        "Origin": {
            "description": "The origin of the model.",
            "$ref": "https://raw.githubusercontent.com/hypar-io/Elements/representation-add-representations/Schemas/GeoJSON/Position.json"
        },
        "Transform": {
            "description": "The transform of the model.",
            "$ref": "https://raw.githubusercontent.com/hypar-io/Elements/representation-add-representations/Schemas/Geometry/Transform.json"
        },
        "Elements": {
            "description": "A collection of Elements keyed by their identifiers.",
            "type": "object",
            "additionalProperties": {
                "$ref": "https://raw.githubusercontent.com/hypar-io/Elements/representation-add-representations/Schemas/Element.json"
            }
        }
    },
    "additionalProperties": false
}