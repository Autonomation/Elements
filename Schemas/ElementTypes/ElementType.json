{
    "$id": "https://hypar.io/schemas/elements/ElementTypes/ElementType.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A container of elements.",
    "title": "ElementType",
    "type": "object",
    "discriminator": "discriminator",
    "required": ["Name"],
    "properties": {
        "Id": {
            "type": "string",
